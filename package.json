{
  "name": "plugins-turborepo",
  "author": "jary",
  "scripts": {
    "dev": "turbo run dev",
    "dev:ui": "pnpm --filter @repo/ui dev",
    "dev:project-starter": "pnpm --filter project-starter dev",
    "build": "turbo run build",
    "build:ui": "pnpm --filter @repo/ui build",
    "build:project-starter": "pnpm --filter project-starter build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "lint:ui": "pnpm --filter @repo/ui lint",
    "lint:project-starter": "pnpm --filter project-starter lint",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "check-types": "turbo run check-types",
    "check-types:project-starter": "pnpm --filter project-starter check-types",
    "start:project-starter": "pnpm --filter project-starter preview"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=22.0.0"
  }
}
